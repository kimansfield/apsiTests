#!/bin/bash 
#
#source $HOME/bin/testSuite/configfile
source $HOME/apsiTests/configfile

runCurl()
{
	echo "Curl --socks5-hostname"
	echo ${CLIENT[0]}:1080 ${TARGET[0]}:80/$LOCATION/$FILENAME
	curl --socks5-hostname ${CLIENT[0]}:1080 ${TARGET[0]}:80/$LOCATION/smallfile > $HOME/temp #/tmp/ramdisk/blah
}

runCurl


